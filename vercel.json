{
  "version": 2,
  "builds": [
    {
      "src": "api/share.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/share",
      "dest": "/api/share.js"
    },
    {
      "src": "/.*",
      "dest": "/public/index.html"
    }
  ]
}
